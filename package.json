{
  "name": "react-parcel-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate to start a new React.JS App build with Parcel.JS",
  "main": "index.js",
  "scripts": {
    "start": "parcel --no-source-maps ./src/index.html",
    "build": "parcel build ./src/index.html --public-url ./ --no-cache",
